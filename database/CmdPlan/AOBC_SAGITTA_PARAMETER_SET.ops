.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 13 1 0.123
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 13
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 13
.# 
.# TRACKING
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 14 0 0.123
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 14 3 3
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 14
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 14
.# 
