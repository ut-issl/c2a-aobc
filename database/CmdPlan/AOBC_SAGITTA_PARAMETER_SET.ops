.#
.# ===== SAGITTA CHANGE PARAMETER =====
.# パラメータの変更手順
.# 1. ドライバ内の値を変更する
.# 2. Sagittaに送信する
.# 3. Sagittaに設定されたパラメータを読み取る
.# SWでアサーションはしていない
.# 変更後に読み取ったパラメータが正常かをテレメで確認する
.# 
.# LOG LEVEL
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 3 0 1
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 3 15 2
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 3
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 3
.# 
.# LIMITS
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 5 0 1
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 5 10 0.123
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 5
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 5
.# 
.# MOUNTING
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 6 0 0
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 6 3 1
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 6
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 6
.# 
.# CAMERA
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 9 0 1
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 9 23 1
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 9
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 9
.# 
.# IMAGE PROCESSOR
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 10 0 1
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 10 4 1
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 10
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 10
.# 
.# CENTROIDING
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 11 0 0
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 11 12 1
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 11
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 11
.# 
.# LISA
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 12 0 6
.AOBC_RT.Cmd_DI_SAGITTA_CHANGE_PARAMETER 12 13 0.123
.AOBC_RT.Cmd_DI_SAGITTA_SET_PARAMETER 12
.AOBC_RT.Cmd_DI_SAGITTA_READ_PARAMETER 12
.# 
