.# ===== MPU9250 FILTER 単体試験 =====
.#
.AOBC_RT.Cmd_GENERATE_TLM 0x40 0x31 1 # AOBC_FILTERをDL
.# 初期値が設定どおりであることを確認する
.#
.AOBC_RT.Cmd_GENERATE_TLM 0x40 0x3d 1 # AOBC_FILTERをDL
.# 初期値が設定どおりであることを確認する
.#
.# ----- コマンド試験 -----
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 0 0 1 2 3.0 4.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 0 1 2 3 4.0 5.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 0 2 4 5 6.0 7.0
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x3d 1 # AOBC_FILTERをDL
.# 正しく設定されていることを確認する
.#
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 1 0 2 8 6.0 4.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 1 1 4 10 4.0 5.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 1 2 6 12 6.0 7.0
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x3d 1 # AOBC_FILTERをDL
.# 正しく設定されていることを確認する
.#
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 0 3 2 1 1.0 1.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 0 4 1 1 1.0 1.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 0 5 1 1 1.0 1.0
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x3d 1 # AOBC_FILTERをDL
.# 値が変わらないことを確認する
.#
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 1 3 2 1 1.0 1.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 1 4 1 1 1.0 1.0
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_SPIKE_FILTER_PARAM 1 5 1 1 1.0 1.0
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x3d 1 # AOBC_FILTERをDL
.# 値が変わらないことを確認する
.#
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 0 0 1.0 0.01
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 0 1 2.0 0.02
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 0 2 3.0 0.03
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x31 1 # AOBC_FILTERをDL
.# 正しく設定されていることを確認する
.#
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 1 0 4.0 0.04
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 1 1 5.0 0.05
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 1 2 6.0 0.06
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x31 1 # AOBC_FILTERをDL
.# 正しく設定されていることを確認する
.#
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 0 4 10 0.1
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 0 5 20 0.1
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 0 6 30 0.1
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x31 1 # AOBC_FILTERをDL
.# 値が変わらないことを確認する
 # 
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 1 7 40 0.1
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 1 8 50 0.1
 wait_sec 1
 Cmd_APP_MPU9250_FILTER_SET_ZFILTER_PARAM 1 9 60 0.1
 wait_sec 1
 AOBC_RT.Cmd_GENERATE_TLM 0x40 0x31 1 # AOBC_FILTERをDL
.# 値が変わらないことを確認する
