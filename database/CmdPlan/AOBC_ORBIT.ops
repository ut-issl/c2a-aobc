 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_SGP4_SET_TLE_LINE1 19 0.0 -0.1
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_SGP4_SET_TLE_LINE2 97.9335 4.56 -0.01 80.345 250.456 15.114
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_SGP4_SET_TLE_LINE2 97.9335 4.56 1.01 80.345 250.456 15.114
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_SGP4_SET_TLE_LINE2 97.9335 4.56 0.0 80.345 250.456 -10.0
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
.# ----- Kepler with GPSR -----
.#
.# APP実行時間計測試験
 AOBC_RT.TG_GENERATE_RT_TLM 0x09 # Kepler GPSR実行前
.# APP49(AR_APP_ORBIT_CALCULATOR)の実行時間を記録する
.#
 # OEM7600の電源投入・テレコマ疎通を確認する
.AOBC_RT.APP_PSC_CONTROL_UNREG_PORT 2 1
.# ！！中断！！（緊急時）
.AOBC_RT.APP_PSC_CONTROL_UNREG_PORT 2 0
.#
.# BCで電源ON/OFFを行う場合
.# BCでOEM7600の電源ON
 AOBC_RT.TLCD_DEPLOY_BLOCK 1 40
.# BCでOEM7600の電源OFF
 AOBC_RT.TLCD_DEPLOY_BLOCK 1 52
.#
.# GPSRテレメを用いた伝播を開始する
 AOBC_RT.APP_ORBIT_CALC_SET_METHOD 2
 wait_sec 5
 AOBC_RT.TG_GENERATE_RT_TLM 0x3c # ORBIT2をDL
.# METHODがKEPLER_GPSRになっていることを確認する
.# KEPLER_GPSR.POS/VELがAOCS_MANAGER.POS/VELと比較して異常でないことを確認
.#
.# GPSRテレメに基づく伝播を、AOCS_MANAGERの位置速度として採用する
 AOBC_RT.APP_ORBIT_CALC_SET_GPSR_UPDATE 1
 wait_sec 5
 AOBC_RT.TG_GENERATE_RT_TLM 0x3c # ORBIT2をDL
.# KEPLER_GPSR.GPSR_UPDATEがENABLEになったことを確認
.# KEPLER_GPSR.POS/VELとAOCS_MANAGER.POS/VELが一致することを確認
.#
.# APP実行時間計測試験
 AOBC_RT.TG_GENERATE_RT_TLM 0x09 # Kepler GPSR実行後
.# APP49(AR_APP_ORBIT_CALCULATOR)の実行時間を記録する
.#
.# コマンド試験
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 0 40.0
 wait_sec 1
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 1 0.08
 wait_sec 1
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 2 0.33
 wait_sec 1
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 3 0.33
 wait_sec 1
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 4 0.66
 wait_sec 1
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 5 0.09
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x3c # ORBIT2をDL
.# KEPLER_GPSR.THRESHOLDが更新されていることを確認する
.#
.# コマンドエラー試験
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 0 -1.0
 wait_sec 1
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x3c # ORBIT2をDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 1 1.1
 wait_sec 1
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x3c # ORBIT2をDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 2 -1.0
 wait_sec 1
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x3c # ORBIT2をDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_GPSROP_SET_ORBIT_ELEMENTS_THRESHOLD 3 -1.0
 wait_sec 1
