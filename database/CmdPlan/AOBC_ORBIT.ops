.# ===== ORBIT 単体試験 =====
.#
.AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 初期値が設定どおりであることを確認する
.#
.# ----- 手法選択試験 -----
 AOBC_RT.APP_ORBIT_CALC_SET_METHOD 0
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# KEPLERになっていることを確認
.#
 AOBC_RT.APP_ORBIT_CALC_SET_METHOD 1
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# SGP4になっていることを確認
.#
 AOBC_RT.APP_ORBIT_CALC_SET_METHOD 2
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# KEPLER_GPSRになっていることを確認
.#
 AOBC_RT.APP_ORBIT_CALC_SET_METHOD 3
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# EKFになっていることを確認
.#
 AOBC_RT.APP_ORBIT_CALC_SET_METHOD 4
.# コマンドエラーになることを確認
.#
.# ----- Kepler試験 -----
.#
.# コマンド試験
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 0.0 0.4 1.2 2.0 2458941.112
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# KEPLER.CMDの値のみが更新されていることを確認する
.# 値の反映
 AOBC_RT.APP_KOP_SAVE_ORBIT_ELEMENTS
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# KEPLERの値が更新されていることを確認する
.#
.# コマンドエラー試験
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 5000.0 0.01 0.2 1.2 2.0 2458941.112
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 -0.1 0.2 1.2 2.0 2458941.112
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 1.1 0.2 1.2 2.0 2458941.112
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 0.01 -0.2 1.2 2.0 2458941.112
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# CMDエラー無し、傾斜角が0~2piになっていることを確認
.#
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 0.01 7.0 1.2 2.0 2458941.112
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# CMDエラー無し、傾斜角が0~2piになっていることを確認
.#
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 0.0 0.4 0.2 0.4 -20000.0
.# CMDエラーを確認
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# 値が更新されていないことを確認
.#
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 0.0 0.4 -0.2 -0.4 2458941.0
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# CMDエラー無し、角度が0~2piになっていることを確認
.#
 AOBC_RT.APP_KOP_SET_ORBIT_ELEMENTS 6800.0 0.0 0.4 7.0 7.5 2458941.0
 wait_sec 1
 AOBC_RT.TG_GENERATE_RT_TLM 0x2c # ORBITをDL
.# CMDエラー無し、角度が0~2piになっていることを確認
.#
.# ----- SGP4試験 -----
.#
