.# ===== AOBC SUNSENSOR SELECTOR コマンド試験 =====
.#
.# 初期値が設定どおりであることを確認する
.AOBC_RT.TG_GENERATE_RT_TLM 0x26 # AOBC_CONTROLをDL
.#
.# ----- コマンド試験 -----
 Cmd_SUN_SENSOR_SELECTOR_SET_INTENSITY_THRESHOLD 90 110
 wait_sec 1
.#
.AOBC_RT.TG_GENERATE_RT_TLM 0x26 # AOBC_CONTROLをDL
.# 正しく設定されていることを確認する
.check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.LOWER == 90
 check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.UPPER == 110
.#
.# ----- コマンド試験 -----
 Cmd_SUN_SENSOR_SELECTOR_SET_INTENSITY_THRESHOLD -90 110
 wait_sec 1
.#
.AOBC_RT.TG_GENERATE_RT_TLM 0x26 # AOBC_CONTROLをDL
.# コマンドが通っていないことを確認する
.check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.LOWER != -90
.#
.# ----- コマンド試験 -----
 Cmd_SUN_SENSOR_SELECTOR_SET_INTENSITY_THRESHOLD 90 -110
 wait_sec 1
.#
.AOBC_RT.TG_GENERATE_RT_TLM 0x26 # AOBC_CONTROLをDL
.# コマンドが通っていないことを確認する
.check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.UPPER != -110
.#
.# ----- コマンド試験 -----
 Cmd_SUN_SENSOR_SELECTOR_SET_INTENSITY_THRESHOLD 110 90
 wait_sec 1
.#
.AOBC_RT.TG_GENERATE_RT_TLM 0x26 # AOBC_CONTROLをDL
.# コマンドが通っていないことを確認する
.check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.LOWER != 110
 check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.UPPER != 90
.#
.# ----- コマンド試験 -----
 Cmd_SUN_SENSOR_SELECTOR_SET_INTENSITY_THRESHOLD 1100 1200
 wait_sec 1
.#
.AOBC_RT.TG_GENERATE_RT_TLM 0x26 # AOBC_CONTROLをDL
.# コマンドが通っていないことを確認する
.check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.LOWER != 1100
 check_value AOBC_CONTROL.SUNSENSOR_SELECTOR_INTENSITY_THRESHOLD.UPPER != 1200

